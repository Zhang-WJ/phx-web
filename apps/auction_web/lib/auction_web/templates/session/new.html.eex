<h1>Log in</h1>

<%= form_for @conn, Routes.session_path(@conn, :create), [as: :user], fn f -> %>

   <%= label f, :username %>
   <%= error_tag f, :username %>
   <%= text_input f, :username %>

   <%= label f, :password %>
   <%= error_tag f, :password %>
   <%= password_input f, :password %>

   <div>
       <%= submit "Submit" %>
   </div>
<% end %>
